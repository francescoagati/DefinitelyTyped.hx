typedef RGB = {
	var r : Float;
	var g : Float;
	var b : Float;
};
typedef RGBA = {
	>RGB,
	var a : Float;
};
typedef HSL = {
	var h : Float;
	var s : Float;
	var l : Float;
};
typedef HSLA = {
	>HSL,
	var a : Float;
};
typedef HSV = {
	var h : Float;
	var s : Float;
	var v : Float;
};
typedef HSVA = {
	>HSV,
	var a : Float;
};
